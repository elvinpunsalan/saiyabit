<h1>Search Products</h1>

<% if @products.present? %>
  <%= paginate @products %>
  <div class="row">
    <div class="card-deck-wrapper">
      <div clas="card-deck">
        <%= render @products %>
      </div>
    </div>
  </div>
<% else %>
  <p>Can't find any cases that match the term(s) <%= params[:search] %>.</p>
<% end %>
